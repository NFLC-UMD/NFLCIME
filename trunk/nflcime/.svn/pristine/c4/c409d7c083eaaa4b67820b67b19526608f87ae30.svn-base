*[lang|="din"],
*[lang|="eng"],
*[lang|="fra"],
*[lang|="fra"],
*[lang|="gaz"],
*[lang|="hat"],
*[lang|="hau"],
*[lang|="hrv"],
*[lang|="ibo"],
*[lang|="ita"],
*[lang|="kin"],
*[lang|="mdh"],
*[lang|="mrw"],
