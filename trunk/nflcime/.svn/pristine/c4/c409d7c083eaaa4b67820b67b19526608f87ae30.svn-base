
/* Setting default target language font (for extended latin) */
@font-face {
	font-family: 'DejaVu Sans Embed';
	src: url('DEJAVUS0.eot');
	src: url('DejaVuSans.ttf');
}

*[lang] {
	font-family: 'Lucida Grande', 'Arial Unicode MS', 'Lucida Sans Unicode', 'DejaVu Sans', 'Microsoft Sans Serif', Arial, sans-serif;
	font-size: 1em;
	line-height: 1.5em;
	text-align: left;
	direction: ltr;
}

*[lang|="snd"], .lang-snd {
	font-family: Arial, sans-serif;
	font-size: 1.4em;
	line-height: 1.5em;
	text-align: right;
	direction: rtl;
}

*[lang|="bam"],
*[lang|="bos"],
*[lang|="bul"],
*[lang|="cbk"],
*[lang|="che"],
*[lang|="dan"],
