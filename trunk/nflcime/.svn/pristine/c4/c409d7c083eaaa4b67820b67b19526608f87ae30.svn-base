	font-family: 'Lucida Grande', 'Arial Unicode MS', 'Lucida Sans Unicode', 'DejaVu Sans', 'DejaVu Sans Embed', Arial, sans-serif;
	
}
/* Nastaliq script */
/* Generated by Font Squirrel (http://www.fontsquirrel.com) on June 29, 2012 */
@font-face {
    font-family: 'NastaliqUnicodeNFLCRegular';
    src: url('nastaliq_unicode_nflc_ie6WebFont.eot');
    src: url('nastaliq_unicode_nflc_ie6WebFont.eot?#iefix') format('embedded-opentype'),
         url('nastaliq_unicode_nflc_ie6WebFont.woff') format('woff'),
         url('nastaliq_unicode_nflc_ie6WebFont.ttf') format('truetype'),
         url('nastaliq_unicode_nflc_ie6WebFont.svg#NastaliqUnicodeNFLCRegular') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
	font-family: 'Pak Nastaleeq Embed';
	src: url('Pak Nastaleeq.ttf') format('truetype');
}

@font-face {
    font-family: 'nafeesnastaleeqnflc';
    src: url('Nafees Nastaleeq NFLC.ttf') format('truetype');
}

/* Jameel Noori Nastaleeq */
@font-face {
    font-family: 'Jameel Noori Nastaleeq';
    src: url('Jameel Noori Nastaleeq.ttf') format('truetype');
}
/* Jameel Noori Nastaleeq Kasheeda */
@font-face {
    font-family: 'Jameel Noori Kasheeda';
    src: url('Jameel Noori Nastaleeq Kasheeda.ttf') format('truetype');
}

@font-face {
    font-family: 'Nafees Nastaleeq 102volt';
    src: url('Nafees Nastaleeq 102volt.ttf') format('truetype');
}

/* http://pakdata.com/products/arabic-font-holy-quran */
@font-face {
	font-family: 'PDMS_Saleem_QuranFont';
	src: url('PDMS_Saleem_QuranFont-signed.ttf') format('truetype');
}

*[lang|="bal"], .lang-bal, /* Balochi */
*[lang|="kas"], .lang-kas, /* Kashmiri */
*[lang|="urd"], .lang-urd, /* Urdu */
*[lang|="pan"], .lang-pan,  /* West Punjabi - deprecated */
*[lang|="pnb"], .lang-pnb,  /* West Punjabi */
*[lang|="phr"], .lang-phr,  /* /Pothohari */
*[lang|="skr"], .lang-skr   /* Siraiki */
{
	font-family: 'NastaliqUnicodeNFLCRegular';
	font-size:  26px;
	line-height: 2em;
	text-align: right;
	direction:rtl;
}
/* a hack for Chrome and Safari because Nastaliq Unicode NFLC doesn't render properly on Mac OS X webkit browsers */
@media screen and (-webkit-min-device-pixel-ratio:0) 
{
	*[lang|="bal"], .lang-bal, /* Balochi */
	*[lang|="kas"], .lang-kas, /* Kashmiri */
	*[lang|="urd"], .lang-urd, /* Urdu */
	*[lang|="pan"], .lang-pan,  /* West Punjabi */
	*[lang|="phr"], .lang-phr,  /* /Pothohari */
	*[lang|="skr"], .lang-skr   /* Siraiki */
	{
		font-family: 'PDMS_Saleem_QuranFont';
		font-size:  28px !important;
		line-height: 1.4em !important;
	}
}

/* Ethiopic (Amharic and Tigrinya) */
@font-face {
    font-family: "Ethiopia Jiret Embed";
    src: url('jiret.ttf') format("opentype"), url('JIRET.eot');
    font-style:  normal;
    font-weight: normal;
}
*[lang|="amh"], .lang-amh, /* Amharic */
*[lang|="tir"], .lang-tir  /* Tigrinya */
{
	font-family: 'Ethiopia Jiret', 'Ethiopia Jiret Embed';
	font-size:  20px;
	line-height: 1.2em;
}

/* Arabic Naskh  */
/* Generated by Font Squirrel (http://www.fontsquirrel.com) on June 28, 2012 */
@font-face {
    font-family: 'KacstBookNFLCMedium';
    src: url('kacstbook_nflcWebFont.eot');
    src: url('kacstbook_nflcWebFont.eot?#iefix') format('embedded-opentype'),
         url('kacstbook_nflcWebFont.woff') format('woff'),
         url('kacstbook_nflcWebFont.ttf') format('truetype'),
         url('kacstbook_nflcWebFont.svg#KacstBookNFLCMedium') format('svg');
    font-weight: normal;
    font-style: normal;

}


*[lang|="ayl"], .lang-ayl, /* Arabic, Libyan */
*[lang|="arz"], .lang-arz, /* Arabic, Egyptian */
*[lang|="afb"], .lang-afb, /* Arabic, Gulf */
*[lang|="mey"], .lang-mey, /* Arabic, Hassaniyya */
*[lang|="acm"], .lang-acm, /* Arabic, Iraqi */
*[lang|="apc"], .lang-apc, /* Arabic, Levantine */
*[lang|="arb"], .lang-arb, /* Arabic, Mod. Std. */
*[lang|="ary"], .lang-ary, /* Arabic, Moroccan */
*[lang|="apd"], .lang-apd, /* Arabic, Sudanese */
*[lang|="gbz"], .lang-gbz, /* Dari, Zoroastrian */
*[lang|="prs"], .lang-prs, /* Dari */
*[lang|="kur"], .lang-kur, /* Kurdish, Sorani */
*[lang|="pus"], .lang-pus, /* Pashto */
*[lang|="pus-AF"], .lang-pus-AF, /* Pashto Afghani*/
*[lang|="pus-PK"], .lang-pus-PK, /* Pashto Pakistani */
*[lang|="pur"], .lang-pur, /* Persian */
*[lang|="fas"], .lang-fas,  /* Persian */
*[lang|="acq"], .lang-acq  /* Arabic, Taï¿½izzi-Adeni Spoken (Yemeni) */
{
	font-family: KacstBookNFLCMedium, 'Geeza Pro', 'Lucida Sans Unicode', Arial, sans-serif;
	font-size:  20px;
	line-height: 1.8em;
	text-align: right;
	direction: rtl;
}

/* Gujarati */
@font-face {
	font-family: 'Shruti Embed';
	src: url('Shruti.ttf');
}

*[lang|="guj"], .lang-guj /* Gujarati */
{
	font-family: 'Shruti', 'Shruti Embed', 'Gujarati MT', 'Arial Unicode MS';
	font-size: 19px;
}

/* Devanagari script */

@font-face {
	font-family: 'Jaipur Mac Embed';
	src: url('Jaipur Unicode NFLC Mac.ttf') format('truetype');
}

@font-face {
	font-family: 'Jaipur Embed';
	src: local("Jaipur Unicode NFLC"), local('Jaipur'), url('Jaipur Unicode NFLC.ttf') format('truetype'), url('JAIPUR.eot'), url('Jaipur Unicode NFLC Mac.ttf') format('truetype');
}

*[lang|="hin"], .lang-hin /* Hindi */
{
	font-family: 'Akshar Unicode', 'Jaipur Unicode NFLC', 'Devanagari MT', 'Jaipur', 'Jaipur Embed', 'Arial Unicode MS', sans-serif;
	font-size: 16px;
}

/* Bengali */
@font-face {
	font-family: 'Rupali Embed';
	src: url('Rupali.ttf') format('truetype'), url('RUPALI.eot');
}

*[lang|="ben"], .lang-ben /* Bengali */
{
	font-family: 'Siyam Rupali', Rupali, 'Bangla NFLC', "Rupali Embed", sans-serif;
	font-size: 17px;
}

/* Burmese */
@font-face {
	font-family: 'Padauk Embed';
	src: url('Padauk.ttf') format('truetype'), url('PADAUKOT.eot');
}

@font-face {
	font-family: 'SgawOT Embed';
	src: url('SgawOT.ttf') format('opentype');
}

*[lang|="mya"], .lang-mya /* Burmese */
{
	font-family: 'Padauk Embed';
	font-size:  20px;
	line-height: 1.5em;
}

/* Hebrew */
*[lang|="heb"], .lang-heb {
	font-size: 15px;
	text-align: right;
	direction: rtl;
}

/* Thai */
@font-face {
	font-family: 'Garuda Embed';
	font-weight: bold;
	font-style: italic;
	src: url('Garuda-BoldOblique.ttf');
}

@font-face {
	font-family: 'Garuda Embed';
	font-weight: bold;
	font-style: normal;
	src: url('Garuda-Bold.ttf');
}

@font-face {
	font-family: 'Garuda Embed';
	font-weight: normal;
	font-style: italic;
	src: url('Garuda-Oblique.ttf');
}

@font-face {
	font-family: 'Garuda Embed';
	font-weight: normal;
	font-style: normal;
	src: url('Garuda.ttf');
}

*[lang|="tha"], .lang-tha /* Thai */
{
	font-family: Thonburi, Garuda, "Garuda Embed", "Arial Unicode MS", Tahoma;
	font-size: 17px;
	line-height: 1.4em;
	text-align: left;	
	direction: ltr;
}
